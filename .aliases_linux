#!/usr/bin/env bash

# Task management
alias f="fg"
alias j="jobs"

# Date

# Get week number
alias week='date +%V'

## Python
alias py="python"
alias ipy="ipython"

## Git
alias log="git log"
alias add="git add"
alias commit="git commit"
alias status="git status"
alias s="git status"
alias pull="git pull"
alias push="git push"
alias branch="git branch"
alias checkout="git checkout"
alias gd="git diff"

alias update="sudo apt update"
